{
  "name": "nuxt-tools-experiments",
  "version": "1.0.0",
  "description": "Репозиторий для тестов с Nuxt",
  "repository": "https://github.com/newbornfrontender/nuxt-tools-experiments.git",
  "author": "Infant Frontender <newbornfrontender@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "stylelint": "stylelint **/*.{css,vue}",
    "stylelint:check": "stylelint-config-prettier-check",
    "stylelint:fix": "yarn stylelint --fix",
    "eslint": "eslint --ext js,jsx,vue .",
    "eslint:check": "eslint --print-config . | eslint-config-prettier-check",
    "eslint:fix": "yarn eslint --fix",
    "prettier": "prettier --no-editorconfig **/*.{css,js,jsx,vue} !.nuxt/** !dist/**",
    "prettier:list": "yarn prettier --list-different",
    "prettier:write": "yarn prettier --write",
    "test:unit": "jest"
  },
  "keywords": [
    "nuxt",
    "eslint",
    "stylelint",
    "jest",
    "prettier"
  ],
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "@nuxtjs/pwa": "^2.6.0",
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "jest": "^23.6.0",
    "jest-transform-stub": "^1.0.0",
    "prettier": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "vue-jest": "^2.6.0"
  }
}
